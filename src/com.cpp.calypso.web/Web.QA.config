{'VIEW':'INDEX,GET,BUSCAR,OBTENER,LIST,BUSQUEDA,DETAILS','EDIT':'ACTUALIZAR,UPDATE','DELETE':'BORRAR,ELIMINAR','CREATE':'NEW,NUEVO'}
       
