@section page_title
{
    <i class="fa fa-tag"></i> Carga de Preciario   XLS
    <h2>Carga Masiva Items Preciario</h2>
}

@{
    ViewBag.Title = "CargaExcel";
}

@if (ViewBag.Error != null)
{
    <div class="alert alert-danger" role="alert">
        <div class="row">

            @ViewBag.Error
        </div>
    </div>

}
@if (ViewBag.Msg != null)
{
    <div class="alert alert-success" role="alert">
        @ViewBag.Msg
    </div>
}
<div align="right">
@Html.ActionLink("Regresar", "Details", "Preciario", new { id = @ViewBag.idpreciario}, new { @class = " btn btn-primary" })
</div>
@using (Html.BeginForm("CargaExcel", "Preciario", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <input type="file" name="UploadedFile" required="" />
    <input type="submit" value="Cargar" class="btn btn-primary" />
}

